{
  "name": "pay-transparency-admin",
  "version": "1.0.0",
  "private": true,
  "description": "pay transparency admin",
  "scripts": {
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "test:cov": "vitest run --mode test --coverage",
    "serve": "vite --host --port 8084",
    "build": "vite build",
    "lint:fix": "eslint --ext vue,js,ts,tsx src --fix",
    "prettier:write": "prettier src --write"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^2.1.0",
    "@braintree/sanitize-url": "^7.0.0",
